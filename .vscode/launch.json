  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CI CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "ci",
        "terraform-format",
        "-c",
        "symphony.yaml",
        "-l",
        "level0",
        "--debug"
      ]
    },
    {
      "name": "Debug CD CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "cd",
        "--level",
        "level0",
        "-c",
        "./local/symphony.yaml",
        "-a",
        "init"
      ]
    },
    {
      "name": "Debug Root",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": []
    }
  ]
}
